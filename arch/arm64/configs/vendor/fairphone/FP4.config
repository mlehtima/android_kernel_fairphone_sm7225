# Append commit sha1 to localversion
CONFIG_LOCALVERSION_AUTO=y

# Audio
CONFIG_SND_SMARTPA_AW882XX=y
CONFIG_T2M_SND_FP4=y

# Board
CONFIG_FP4_BOARD_ID=y
CONFIG_MACH_FAIRPHONE_FP4=y

# Charger
# CONFIG_SMB1390_CHARGE_PUMP_PSY is not set
# CONFIG_SMB1355_SLAVE_CHARGER is not set
CONFIG_BATTERY_BQ27541_HDQ=y
CONFIG_BATTERY_QNOVO_CHARGE_ALGO=y
CONFIG_TCT_PM7250_COMMON=y

# CPU frequency stats
CONFIG_CPU_FREQ_STAT=y

# DT overlay
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Exfat
CONFIG_EXFAT_FS=y
CONFIG_NLS_UTF8=y

# Fingerprint
# CONFIG_FPR_FPC is not set
CONFIG_CHIPONE_FINGERPRINT=y

# NFC
# CONFIG_NFC_NQ is not set
CONFIG_NFC_ST21NFC=y

# Touchscreen
# CONFIG_SECURE_TOUCH_SYNAPTICS_DSX is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_REFLASH is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_RECOVERY is not set
# CONFIG_TOUCHSCREEN_FTS is not set
CONFIG_TOUCHSCREEN_HIMAX_CHIPSET=y
CONFIG_TOUCHSCREEN_HIMAX_COMMON=y
CONFIG_TOUCHSCREEN_HIMAX_IC_HX83112=y
CONFIG_TOUCHSCREEN_HIMAX_DEBUG=y
CONFIG_TOUCHSCREEN_HIMAX_INSPECT=y

# USB
# CONFIG_USB_EHCI_HCD is not set
# CONFIG_USB_EHCI_HCD_PLATFORM is not set
# CONFIG_USB_OHCI_HCD is not set
# CONFIG_USB_OHCI_HCD_PLATFORM is not set
# CONFIG_USB_ISP1760 is not set
# CONFIG_USB_ISP1760_HOST_ROLE is not set
# CONFIG_MSM_HSUSB_PHY is not set
# CONFIG_USB_QCOM_EMU_PHY is not set

# USB debugging
# CONFIG_QCOM_EUD is not set

# Vibrator
# CONFIG_LEDS_QPNP_VIBRATOR_LDO is not set
CONFIG_AW8695_HAPTIC=y

# WiFi
CONFIG_QCA_CLD_WLAN=m



# hybris Pie requires
CONFIG_SECURITY_YAMA=y
CONFIG_SECURITY_YAMA_STACKED=y
CONFIG_BT_HCIUART=y
CONFIG_BT_HCIUART_H4=y
CONFIG_FW_LOADER_USER_HELPER=y

# hybris flags from Oreo
CONFIG_AUTOFS4_FS=y
CONFIG_BLK_CGROUP=y
CONFIG_BT_RFCOMM=y
CONFIG_CGROUP_DEVICE=y
CONFIG_CGROUP_PERF=y
CONFIG_CHECKPOINT_RESTORE=y
CONFIG_DEVTMPFS=y
CONFIG_DEVTMPFS_MOUNT=y
CONFIG_DUMMY=n
CONFIG_FANOTIFY=y
CONFIG_FHANDLE=y
CONFIG_IPC_NS=y
CONFIG_NETFILTER_NETLINK_ACCT=y
CONFIG_NETFILTER_XT_MATCH_MULTIPORT=y
CONFIG_NETFILTER_XT_MATCH_NFACCT=y
CONFIG_CGROUP_NET_PRIO=y
CONFIG_NET_CLS_CGROUP=y
CONFIG_PID_NS=y
CONFIG_SECURITY_SELINUX_BOOTPARAM=y
CONFIG_SYSVIPC=y
CONFIG_UTS_NS=y
